       <REFERENCE>X</REFERENCE>
       <TYP>/SCWM/LGNUM</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IV_KEEP_PICKHU</PARAMETER>
       <DEFAULT>SPACE</DEFAULT>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>XFELD</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IV_WRITE_LOG</PARAMETER>
       <DEFAULT>ABAP_TRUE</DEFAULT>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>XFELD</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IS_WO_TEMPLATE</PARAMETER>
